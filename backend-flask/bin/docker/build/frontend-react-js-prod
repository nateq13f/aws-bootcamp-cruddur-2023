--build-arg REACT_APP_AWS_USER_POOLS_ID="us-east-1_YIsZZQ2ZU" \
--build-arg REACT_APP_CLIENT_ID="43g33jegd4tre25a7nip7ggohb" \
-t frontend-react-js \
-f Dockerfile.prod \
.